{
  "name": "cs-quiz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings 0",
    "type-check": "tsc --noEmit",
    "check": "npm run lint && npm run type-check",
    "import-questions": "ts-node -P scripts/tsconfig.scripts.json scripts/ai-regenerate/import.ts",
    "import-questions:dry": "ts-node -P scripts/tsconfig.scripts.json scripts/ai-regenerate/import.ts --dry-run",
    "add-topic": "ts-node -P scripts/tsconfig.scripts.json scripts/ai-regenerate/add-topic.ts",
    "generate-daily": "ts-node -P scripts/tsconfig.scripts.json scripts/generate-daily-questions.ts",
    "generate-daily:week": "ts-node -P scripts/tsconfig.scripts.json scripts/generate-daily-questions.ts 7",
    "generate-daily:month": "ts-node -P scripts/tsconfig.scripts.json scripts/generate-daily-questions.ts 30"
  },
  "prisma": {
    "seed": "ts-node -P scripts/tsconfig.scripts.json prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}